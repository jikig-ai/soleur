{
  "env": {
    "CLAUDE_CODE_EFFORT_LEVEL": "high"
  },
  "extraKnownMarketplaces": {
    "soleur": {
      "source": {
        "source": "github",
        "repo": "jikig-ai/soleur"
      }
    }
  },
  "enabledPlugins": {
    "soleur@soleur": true
  },
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$CLAUDE_PROJECT_DIR/.claude/hooks/session-start.sh"
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/guardrails.sh"
          }
        ]
      }
    ]
  }
}
